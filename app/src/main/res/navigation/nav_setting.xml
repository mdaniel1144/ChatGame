<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_setting"
    app:startDestination="@id/fragment_SettingMenu">
    <fragment
        android:id="@+id/fragment_SettingMenu"
        android:name="com.example.gamechats_final.Fragments.fragment_SettingMenu"
        android:label="fragment_setting_menu"
        tools:layout="@layout/fragment_setting_menu" >
        <action
            android:id="@+id/action_fragment_SettingMenu_to_fragment_SettingChangePassword"
            app:destination="@id/fragment_SettingChangePassword" />
        <action
            android:id="@+id/action_fragment_SettingMenu_to_fragment_setting_deatils"
            app:destination="@id/fragment_setting_deatils" />
        <action
            android:id="@+id/action_fragment_SettingMenu_to_fragment_SettingUploadImage"
