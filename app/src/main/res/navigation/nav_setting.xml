            android:id="@+id/action_fragment_setting_deatils_to_fragment_SettingMenu"
            app:destination="@id/fragment_SettingMenu" />
    </fragment>
    <fragment
        android:id="@+id/fragment_SettingUploadImage"
        android:name="com.example.gamechats_final.Fragments.fragment_SettingUploadImage"
        android:label="fragment_setting_uploadimage"
        tools:layout="@layout/fragment_setting_uploadimage" >
        <action
            android:id="@+id/action_fragment_SettingUploadImage_to_fragment_SettingMenu"
            app:destination="@id/fragment_SettingMenu" />
    </fragment>
</navigation>